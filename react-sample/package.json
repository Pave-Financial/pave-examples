{
  "name": "pave-dashboard",
  "version": "1.3.0",
  "description": "Pave Example Dashboard",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "apexcharts": "3.26.0",
    "availity-reactstrap-validation": "2.7.1",
    "bootstrap": "4.6.0",
    "chokidar-cli": "2.1.0",
    "classnames": "2.2.6",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.10.4",
    "faker": "^5.5.1",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "3.6.0",
    "lodash": "^4.17.21",
    "node-sass": "7.0.0",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "numeral": "^2.0.6",
    "postcss-cli": "6.1.3",
    "react": "16.13.1",
    "react-apexcharts": "1.3.7",
    "react-app-polyfill": "1.0.6",
    "react-bootstrap-table-next": "4.0.3",
    "react-bootstrap-table2-overlay": "^2.0.0",
    "react-bootstrap-table2-paginator": "2.1.2",
    "react-bootstrap-table2-toolkit": "2.1.3",
    "react-dom": "16.13.1",
    "react-feather": "2.0.9",
    "react-input-mask": "2.0.4",
    "react-perfect-scrollbar": "1.5.8",
    "react-redux": "7.2.3",
    "react-redux-toastr": "7.6.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "4.3.0",
    "react-transition-group": "4.4.1",
    "reactstrap": "8.9.0",
    "redux": "4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {}
}
